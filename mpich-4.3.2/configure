    ac_fc_srcext="f"
    cat > conftest.$ac_ext <<_ACEOF

              program main

      end

_ACEOF
if ac_fn_fc_try_compile "$LINENO"; then :

        { $as_echo "$as_me:${as_lineno-$LINENO}: result: f" >&5
$as_echo "f" >&6; }

else

